<h1>New Event</h1>
<%= form_for @event do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br><br>

  <%= f.label :description %>
  <%= f.text_area :description %><br><br>

  <%= f.label :location %>
  <%= f.collection_select :location_id, Location.all, :id, :name %><br><br>

  <%= f.label :event_type %>
  <%= f.collection_select :event_type, Event.all, :event_type, :event_type %><br><br>
  <%= f.submit %>
<% end %>
